print: cfunc (format: ^byte, ...) -> void;
alloc: cfunc (bytes: int) -> ^byte;
quit:  cfunc (key: int) -> void;

main: () -> void {
	
	arr: ^^int;
	dimx: int;
	dimy: int;
	i: int;
	j: int;

	dimx = 10;
	dimy = 20;

	arr = #^^int alloc(dimx * 8);

	for (i = 0; i < dimx; i += 1) {
		$(arr + i*8) = #^int alloc(dimy * 8);
		for (j = 0; j < dimy; j += 1) {
			$($(arr + i*8) + j*8) = i + j;
		}
	}

}
