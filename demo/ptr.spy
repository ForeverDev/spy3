print: foreign (format: ^byte, ...) -> int;
alloc: foreign (bytes: int) -> ^byte;

struct Meme {
	field: int;
};

struct Foo {
	a: int;
	b: int;
	m: Meme;
};

main: () -> void {

	foo: [1]Foo;
	
	foo[0].a = 10;
	foo[0].b = 20;
	foo[0].m.field = 30;
	
	print("%d %d %d\n", foo[0].a, foo[0].b, foo[0].m.field);
}
