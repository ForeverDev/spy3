jmp main

print: 
	db "print\0"

factorial:
	iarg	0
	ipush	1
	icmp
	jgt		.continue
	ipush	1
	jmp		.done
.continue:
	iarg	0
	iarg	0
	ipush	1
	isub
	call	factorial, 1
	imul
.done:
	iret

main:
	ipush	5
	call	factorial, 1
	ipush	5
	ipush	.output_string
	cfcall	print, 3
	pop

	
	ipush	6
	call	factorial, 1
	ipush	6
	ipush	.output_string
	cfcall	print, 3
	pop

	exit
.output_string:
	db "factorial of %d is: %d\n\0"
