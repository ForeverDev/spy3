struct Name {
	first: ^byte;
	middle: ^byte;
	last: ^byte;
};

struct Person {
	age: int;
	name: Name;
};

struct NestedFunction {
	function: (a: Person) -> void;
};

do_something: (person: Person) -> void {
}

main: () -> void {
	
	nest: NestedFunction;
	david: Person;

	nest.function = do_something;

	david.age = 16;
	david.name.first = "David";
	david.name.middle = "Bradford";
	david.name.last = "Wilson";

	nest.function(david);
	
}

